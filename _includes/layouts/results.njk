---
layout: layouts/base.njk
pagination:
    data: results.f2021
    size: 1
    alias: results
permalink: "{{ link }}/{{ results.team | slug }}/"
templateClass: result-page
---
<article class="oil-page">

<h1 class="post-title">{{ results.team }} {% if results.topTen %}<i class='award'>‚≠ê</i>{% endif %}
        {% if results.award == 1 %}<i class='award'>ü•á</i>{% endif %}
        {% if results.award == 2 %}<i class='award'>ü•à</i>{% endif %}
        {% if results.award == 3 %}<i class='award'>ü•â</i>{% endif %}</h1>
<figure class="video-container">
<iframe loading="lazy" src="{{ results.video }}" width="100%" height="100%" frameborder="0" webkitallowfullscreen="" mozallowfullscreen="" allowfullscreen=""></iframe>
</figure>


{%- set nextPost = collections.posts | getNextCollectionItem(page) %}
{%- set previousPost = collections.posts | getPreviousCollectionItem(page) %}
{%- if nextPost or previousPost %}
<hr>
<ul>
  {%- if nextPost %}<li>Next: <a href="{{ nextPost.url | url }}">{{ nextPost.data.title }}</a></li>{% endif %}
  {%- if previousPost %}<li>Previous: <a href="{{ previousPost.url | url }}">{{ previousPost.data.title }}</a></li>{% endif %}
</ul>
{%- endif %}
</article>
