---
layout: layouts/base.njk
templateClass: tmpl-page

---
<section>

<h1 class="post-title">{{ title }}</h1>

{{ content | safe }}

<ul class="result-list">
  {% for item in results[list] %}
    <li class="media-box">
      <a href="{{ item.team | url }}">
        {{ item.team }}
        </a>
      </li>
    {% endfor %}
  </ul>
<hr>

</section>
